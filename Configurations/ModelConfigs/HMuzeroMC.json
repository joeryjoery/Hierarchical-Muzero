{
  "name": "Hierarchical_Base",
  "algorithm": "HIERARCHICAL_MUZERO",
  "architecture": "Hierarchical_Gym",
  "goal_architecture": "Gym",
  "action_architecture": "Gym",

  "args": {
    "num_selfplay_iterations": 500,
    "num_episodes": 20,
    "num_gradient_steps": 40,
    "max_episode_moves": 500,
    "max_trial_moves": 500,
    "pitting": true,
    "pitting_trials": 10,
    "pit_acceptance_ratio": 0.0,
    "dirichlet_alpha": 0.3,
    "exploration_fraction": 0.25,
    "max_buffer_size": 200000,
    "num_MCTS_sims": 11,
    "prioritize": false,
    "prioritize_alpha": 0.5,
    "prioritize_beta": 1,
    "latent_decoder": false,
    "plan_actions": false,
    "plan_goals": true,
    "goal_error": 0.05,
    "goal_horizon": 5,
    "subgoal_testing": 0.3,
    "subgoal_sampling": 0.5,
    "subgoal_penalty": -30,
    "K": 3,
    "n_steps": 11,
    "c_pw": 1,
    "alpha_pw": 0.5,
    "c1": 1.25,
    "c2": 19652,
    "gamma": 0.997,

    "minimum_reward": null,
    "maximum_reward": null,

    "checkpoint": "./out/hierarchical/mc_out_long_run/",
    "load_model": false,
    "load_folder_file": ["./out/hierarchical/mc_out_long_run/", "latest.pth.tar"],
    "selfplay_buffer_window": 10,

    "goal_explore_schedule": {
      "method": "stepwise",
      "by_weight_update": true,
      "schedule_points": [[0, 1]]
    },
    "action_explore_schedule": {
      "method": "linear",
      "by_weight_update": true,
      "schedule_points": [[0, 0.2], [5000, 0.1], [15000, 0.05]]
    }
  },

  "net_args": {
    "optimizer": {
      "method": "adam",
      "lr_init": 0.02,
      "momentum": 0.9
    },
    "goal_space": {
      "latent": false
    },
    "l2": 1e-4,
    "dynamics_penalty": 0.0,
    "entropy_penalty": 0.15,
    "dropout": 0.0,
    "gamma": 0.95,
    "norm_clip": 10,
    "target_frequency": 5,
    "batch_size": 128,
    "latent_depth": 8,
    "num_channels": 256,
    "num_towers": 3,
    "num_dense": 3,
    "actor_size": 3,
    "actor_bound": 5,
    "size_dense": 64,
    "activation": "relu",
    "support_size": 15,
    "observation_length": 1
  }
}